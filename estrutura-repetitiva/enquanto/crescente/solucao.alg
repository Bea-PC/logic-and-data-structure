Algoritmo "crescente"

Var

   num1,num2: real

Inicio

   escreval("Digite dois numeros:")
   leia(num1)
   leia(num2)
   
   enquanto num1 <> num2 faca
      se num1 < num2 entao
         escreval("CRESCENTE!")
      senao
         escreval("DECRESCENTE!")
      fimse
      
      escreval("Digite outros dois numeros:")
      leia(X)
      leia(Y)
   fimenquanto

Fimalgoritmo